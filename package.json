{
  "name": "react-query-kit",
  "version": "1.2.1",
  "description": "A simplest way to create react-query without considering the consistence of queryKey or queryFn ",
  "author": "liaoliao666",
  "repository": "liaoliao666/react-query-kit",
  "homepage": "https://github.com/liaoliao666/react-query-kit",
  "main": "lib/index.js",
  "unpkg": "dist/react-query-kit.development.js",
  "types": "types/index.d.ts",
  "module": "es/index.js",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.10",
    "@babel/plugin-transform-runtime": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@rollup/plugin-replace": "^4.0.0",
    "@tanstack/react-query": "^4.2.3",
    "@types/jest": "^28.1.6",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "babel-jest": "^28.1.3",
    "babel-plugin-const-enum": "^1.2.0",
    "babel-plugin-transform-async-to-promises": "^0.8.18",
    "cross-env": "^7.0.3",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "replace": "^1.2.1",
    "rollup": "^2.77.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-size": "^0.2.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-visualizer": "^5.7.1",
    "ts-jest": "^28.0.7",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "@tanstack/react-query": "*"
  },
  "peerDependenciesMeta": {
    "@tanstack/react-query": {
      "optional": true
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "yarn build:commonjs && yarn build:es && yarn build:umd && yarn build:types",
    "build:commonjs": "rimraf ./lib && cross-env BABEL_ENV=commonjs babel --extensions .ts,.tsx --ignore ./src/**/*.test.tsx ./src --out-dir lib",
    "build:es": "rimraf ./es && babel --extensions .ts,.tsx --ignore ./src/**/*.test.tsx ./src --out-dir es",
    "build:umd": "rimraf ./dist && cross-env NODE_ENV=production rollup -c && rollup-plugin-visualizer stats.json",
    "build:types": "rimraf ./types && tsc --project ./tsconfig.types.json && replace 'import type' 'import' ./types -r --silent && replace 'export type' 'export' ./types -r --silent",
    "stats": "open ./stats.html",
    "eslint": "eslint --fix '*.{js,json}' '{src,tests,benchmarks}/**/*.{ts,tsx}'",
    "test": "jest"
  },
  "dependencies": {
    "@babel/runtime": "^7.18.9"
  },
  "files": [
    "dist",
    "es",
    "lib",
    "types"
  ],
  "keywords": [
    "react",
    "react query"
  ]
}
